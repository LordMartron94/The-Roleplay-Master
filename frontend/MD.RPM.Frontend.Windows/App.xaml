             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:windowsInterface="clr-namespace:MD.RPM.Frontend.Windows"
             xmlns:viewModels="clr-namespace:MD.RPM.Frontend.Windows.ViewModels">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <viewModels:Bootstrapper x:Key="Bootstrapper" />
                </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>